<html>
  <!-- head -->
  <%- include('partials/head'); %>

  <body class="is-preload">
    <!-- Wrapper -->
    <div id="wrapper">
      <!-- Main -->
      <div id="main">
        <div class="inner">
          <!-- Header -->
          <%- include('partials/header'); %>

          <!-- Banner -->
          <section id="banner">
            <div class="content">
              <header class="major">
                <h2><%= title %></h2>
              </header>
              <p>
                You can send us your music to be shared with the community.
                Please get in touch and we will reach out to you soon. Thank
                you!
              </p>

              <!-- Contact form -->
              <%- include('partials/contact-form'); %>

              <p>You can also support Partitas Music by becoming a Patreon.</p>

              <p>
                <a href="https://www.patreon.com/partitasmusic" target="_blank">
                  <button
                    class="button primary icon solid fa-external-link-alt"
                    type="submit"
                  >
                    become a patreon
                  </button>
                </a>
              </p>

              <p>
                This website is an open source project. You can contribute with
                code
                <a
                  href="https://github.com/marcospozzo/partitasmusic/issues"
                  target="_blank"
                  >here</a
                >.
              </p>
            </div>
            <span class="image object">
              <img
                src="./images/contribute.jpg"
                alt="Music score with the beginning of a piece by Johann Sebastian Bach."
              />
            </span>
          </section>
        </div>
      </div>

      <!-- Sidebar -->
      <%- include('partials/sidebar'); %>
    </div>

    <!-- Scripts -->
    <%- include('partials/scripts'); %>
  </body>
</html>
