<html>
  <!-- head -->
  <%- include('partials/head'); %>

  <body class="is-preload">
    <!-- Wrapper -->
    <div id="wrapper">
      <!-- Main -->
      <div id="main">
        <div class="inner">
          <!-- Header -->
          <%- include('partials/header'); %>

          <!-- Banner -->
          <section id="banner">
            <div class="content">
              <header class="major">
                <h2><%= title %></h2>
              </header>
              <p>
                We look forward to receiving your message. Ideas and suggestions
                are most welcome too!
              </p>

              <!-- Contact form -->
              <%- include('partials/contact-form'); %>
              <ul class="icons">
                <li>
                  <a
                    href="https://facebook.com/partitasmusic"
                    target="_blank"
                    class="icon brands fa-facebook-f"
                    ><span class="label">Facebook</span></a
                  >
                </li>
                <li>
                  <a
                    href="https://instagram.com/partitasmusic"
                    target="_blank"
                    class="icon brands fa-instagram"
                    ><span class="label">Instagram</span></a
                  >
                </li>
                <li>
                  <a
                    href="https://twitter.com/gcaphorisms"
                    target="_blank"
                    class="icon brands fa-twitter"
                    ><span class="label">Twitter</span></a
                  >
                </li>
              </ul>
            </div>
            <span class="image object">
              <img
                src="./images/contact.png"
                alt="A zenith picture of seven guitarists with three colored lines connecting them."
              />
            </span>
          </section>
        </div>
      </div>

      <!-- Sidebar -->
      <%- include('partials/sidebar'); %>
    </div>

    <!-- Scripts -->
    <%- include('partials/scripts'); %>
  </body>
</html>
