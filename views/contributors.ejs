<html>
  <!-- head -->
  <%- include('partials/head'); %>

  <body class="is-preload">
    <!-- Wrapper -->
    <div id="wrapper">
      <!-- Main -->
      <div id="main">
        <div class="inner">
          <!-- Header -->
          <%- include('partials/header'); %>

          <section>
            <header class="major">
              <h2>Music Contributors</h2>
            </header>

            <div class="contributors">
              <!-- Table -->
              <div class="table-wrapper">
                <h3>Groups & Ensembles</h3>
                <table>
                  <tbody>
                    <% groups.forEach(function (group) { %>
                    <tr>
                      <td>
                        <a href="/contributions/<%= group.path %>"
                          ><%= group.name %></a
                        >
                      </td>
                    </tr>
                    <% }); %>
                  </tbody>
                </table>
              </div>

              <!-- Table -->
              <div class="table-wrapper">
                <h3>Individuals</h3>
                <table>
                  <tbody>
                    <% individuals.forEach(function (individual) { %>
                    <tr>
                      <td>
                        <a href="/contributions/<%= individual.path %>"
                          ><%= individual.name %></a
                        >
                      </td>
                    </tr>
                    <% }); %>
                  </tbody>
                </table>
              </div>
            </div>
          </section>
        </div>
      </div>

      <!-- Sidebar -->
      <%- include('partials/sidebar'); %>
    </div>

    <!-- Scripts -->
    <%- include('partials/scripts'); %>
  </body>
</html>
